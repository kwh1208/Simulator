<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="leftPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="dbps.dbps.controller.UnderTheLineLeftController" layoutX="-5.0" layoutY="382.0" prefHeight="257.0" prefWidth="272.0">
<children>
    <Button layoutX="65.0" layoutY="18.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="115.0" text="화면 켜기" onMouseClicked="#sendDisplayOn"/>
    <Button layoutX="180.0" layoutY="18.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="115.0" text="화면 끄기" onMouseClicked="#sendDisplayOff" />
    <TextField fx:id="timeBoard" alignment="CENTER" layoutX="65.0" layoutY="58.0" prefHeight="30.0" prefWidth="230.0" text="현재 시간" editable="false"/>
    <Button layoutX="65.0" layoutY="98.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="230.0" text="컨트롤러 시간 읽기" onMouseClicked="#getControllerTime"/>
    <Button layoutX="65.0" layoutY="138.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="230.0" text="PC 시간 동기화" onMouseClicked="#synchronizeTime"/>
    <Button layoutX="65.0" layoutY="178.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="컨트롤러 리셋" onMouseClicked="#resetController"/>
    <Button layoutX="185.0" layoutY="178.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="110.0" text="공장 초기화" onMouseClicked="#hardReset"/>
</children>
</Pane>

        <Label layoutX="25.0" layoutY="175.0" text="password"/>
        <TextField fx:id="password" layoutX="113.0" layoutY="167.0" prefHeight="29.0" prefWidth="240.0"/>
        <Button layoutX="25.0" layoutY="219.0" mnemonicParsing="false" onMouseClicked="#set" prefHeight="21.0"
                prefWidth="80.0" text="설정"/>
        <Button layoutX="273.0" layoutY="219.0" mnemonicParsing="false" onMouseClicked="#close" prefHeight="21.0"
                prefWidth="80.0" text="닫기"/>
    </Pane>
</AnchorPane>

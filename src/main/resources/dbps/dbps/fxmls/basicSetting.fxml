<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<?import javafx.scene.control.ComboBox?>
<Pane fx:id="basicPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="dbps.dbps.controller.BasicSettingController" layoutX="19.0" layoutY="57.0" prefHeight="95.0"
      prefWidth="310.0" style="-fx-border-color: #4A4A4A; -fx-border-radius: 10">
    <Label alignment="CENTER" layoutX="85.0" layoutY="-12.0" prefHeight="18.0" prefWidth="140.0" text="%programSetting"
           style="-fx-background-color: #4A4A4A; -fx-padding: 5px; -fx-font-weight: bold; -fx-text-fill: white;  -fx-origin: top;"/>
    <Label layoutX="14.0" layoutY="30.0" text="%protocolFormat" style=""/>
    <ComboBox fx:id="protocolFormat" layoutX="150.0" layoutY="24.0" prefHeight="25.0" prefWidth="150.0"
               value="%ASCiiProtocol" >
        <items>
            <FXCollections fx:factory="observableArrayList">
            </FXCollections>
        </items>
    </ComboBox>
    <Label layoutX="14.0" layoutY="66.0" text="%programLanguage" style=""/>
    <ComboBox fx:id="programLanguage" layoutX="150.0" layoutY="60.0" prefHeight="25.0" prefWidth="150" value="한국어" style="">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="한국어"/>
                <String fx:value="English"/>
            </FXCollections>
        </items>
    </ComboBox>
</Pane>
